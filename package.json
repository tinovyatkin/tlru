{
  "name": "tlru",
  "version": "1.1.0",
  "description": "Time aware least recently used (TLRU) cache for Node",
  "keywords": [
    "LRU",
    "cache",
    "expires",
    "lru-cache",
    "receptacle",
    "setTimeout",
    "time-aware",
    "tlru",
    "ttl",
    "ttu",
    "typescript"
  ],
  "homepage": "https://github.com/tinovyatkin/tlru#readme",
  "bugs": {
    "url": "https://github.com/tinovyatkin/tlru/issues"
  },
  "license": "MIT",
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinovyatkin/tlru.git"
  },
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "dist/*.js",
    "dist/*.ts"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "node --expose-gc --experimental-test-coverage --test __tests__/*.test.ts",
    "lint": "oxlint --tsconfig ./tsconfig.lint.json --type-aware",
    "format": "oxfmt --write .",
    "build": "tsgo",
    "postversion": "git push origin master --tags",
    "prepublishOnly": "tsgo && npm test"
  },
  "dependencies": {
    "fun-dispatcher": "^1.2.6"
  },
  "devDependencies": {
    "@types/node": "24",
    "@typescript/native-preview": "latest",
    "oxfmt": "0",
    "oxlint": "1",
    "oxlint-tsgolint": "0"
  },
  "engines": {
    "node": ">=20"
  }
}
